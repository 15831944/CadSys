@model ExportViewModel
@{
    ViewData["Title"] = "Download";
}

<h2>Files download</h2>
<div>
    <ul>


        @foreach (var f in Model.Files)
        {
            <li>
                <input type="button" value="@f.Display" onclick="location.href='@Url.Action("GetFile", "Export", f )'" />
                @if (f.ClassName.Contains("dxf"))
                {
                    <input type="button" value="Importa in baza NrCadGeneral" onclick="location.href='@Url.Action("ImportNrCadGen", "Export", f )'" />
                }

            </li>
        }
    </ul>
</div>
<div>
    <ul id="messagesList"></ul>
</div>
